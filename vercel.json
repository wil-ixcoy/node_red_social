{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "posts/post.index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/user(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/post(.*)",
      "dest": "/api/post.index.js"
    }
  ]
}
