{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "post/post.index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/auth(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/user(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/post(.*)",
      "dest": "/post/post.index.js"
    }
  ]
}
